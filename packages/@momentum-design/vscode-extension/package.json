{
  "name": "@momentum-design/vscode-extension",
  "displayName": "@momentum-design/vscode-extension",
  "description": "VS Code Extension for Momentum",
  "packageManager": "yarn@3.2.4",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:momentum.start"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "momentum.start",
        "title": "Momentum Design",
        "category": "Momentum Design"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run build",
    "build:ui": "vite build",
    "preview:ui": "concurrently \"yarn run build:ui --watch\" \"vite preview\"",
    "build:ext": "tsc -p tsconfig.extension.json",
    "build": "yarn run build:ui && yarn run build:ext",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "path": "^0.12.7",
    "typescript": "^5.3.3",
    "vite": "^5.2.10"
  },
  "dependencies": {
    "@momentum-ui/react-collaboration": "^26.101.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
