{
  "name": "@momentum-design/components",
  "packageManager": "yarn@3.2.4",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0",
    "yarn": ">=3.0.0"
  },
  "scripts": {
    "analyze": "yarn analyze:prebuild && yarn analyze:postbuild",
    "analyze:postbuild": "echo \"script 'analyze:postbuild' has not been implemented\"",
    "analyze:prebuild": "echo \"script 'analyze:prebuild' has not been implemented\"",
    "dev": "concurrently --names 'manifest,storybook' -c 'blue.bold,magenta.bold' 'yarn:dev:generate-manifest' 'yarn:dev:storybook'",
    "dev:generate-manifest": "custom-elements-manifest analyze --config config/custom-elements-manifest.config.js",
    "dev:storybook": "storybook dev -p 6006",
    "build": "tsc",
    "clean": "echo \"script 'clean' has not been implemented\"",
    "clean:dist": "rimraf ./dist",
    "docs": "echo \"script 'docs' has not been implemented\"",
    "publish": "yarn publish:npmjs",
    "publish:npmjs": "echo \"script 'publish:npmjs' has not been implemented\"",
    "test": "yarn test:prebuild && yarn test:postbuild",
    "test:postbuild": "echo \"script 'test:prebuild' has not been implemented\"",
    "test:prebuild": "echo \"script 'test:prebuild' has not been implemented\"",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@momentum-design/icons": "workspace:^",
    "@momentum-design/tokens": "workspace:^",
    "lit": "^2.7.3"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.8.1",
    "@playwright/test": "^1.28.0",
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/blocks": "^7.0.9",
    "@storybook/manager-api": "^7.0.9",
    "@storybook/theming": "^7.0.9",
    "@storybook/web-components": "^7.0.9",
    "@storybook/web-components-vite": "^7.0.9",
    "@web/dev-server": "^0.1.35",
    "concurrently": "^8.0.1",
    "delay-cli": "^2.0.0",
    "ncp": "^2.0.0",
    "node-html-parser": "^6.1.4",
    "nodemon": "^2.0.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "storybook": "^7.0.9",
    "typescript": "^4.6.4"
  }
}
